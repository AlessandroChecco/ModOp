{
 "background": {"scripts": ["background.js"]},
 "browser_action": {
 "default_icon": "icon-128.png",
 "default_title": "ModOp"
 },
 "name": "ModOp",
 "description": " - Created by Alessandro Checco",
 "homepage_url": "https://github.com/AlessandroChecco/ModOp",
 "icons": {
     "16": "icon-16.png",
     "48": "icon-48.png",
     "128": "icon-128.png" },
 "permissions": [
     "tabs",
     "http://*/*",
     "https://*/*",
	 "notifications",
	 "contextMenus"
 ],
 "version": "0.1",
 "manifest_version": 2,
 "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}